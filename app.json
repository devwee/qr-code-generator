{
  "expo": {
    "name": "QR Code Generator vBETA",
    "slug": "A basic QR Code Generator.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.devwee.qrcodegenerator"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff",
        "package": "com.devwee.qrcodegenerator"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
